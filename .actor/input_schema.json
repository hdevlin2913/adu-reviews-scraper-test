{
    "title": "Scrape data and reviews of places from TripAdvisor",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
       "placesQuery": {
          "title": "List of places to scrape",
          "type": "array",
          "description": "List of places to scrape",
          "prefill": ["Đà Lạt", "Nha Trang", "Vũng Tàu", "Đà Nẵng"],
          "editor": "json"
       },
       "useApifyProxy": {
          "title": "Apify Proxy",
          "type": "boolean",
          "description": "Use Proxies provided by Apify",
          "default": false
       }
    },
    "required": ["placesQuery"]
}
