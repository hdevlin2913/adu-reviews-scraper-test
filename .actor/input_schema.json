{
    "title": "Scrape data from a web page",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
       "placesQuery": {
          "title": "Name of the places to scrape",
          "type": "array",
          "description": "List of places to scrape",
          "prefill": ["Đà Lạt", "Nha Trang", "Vũng Tàu", "Đà Nẵng"],
          "editor": "requestListSources"
       },
       "useApifyProxy": {
          "title": "Apify Proxy",
          "type": "boolean",
          "description": "Use Proxies provided by Apify",
          "default": false
       }
    },
    "required": ["url"]
}
