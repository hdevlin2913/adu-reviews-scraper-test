{
    "title": "Scrape data and reviews of places from TripAdvisor",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
       "query": {
          "title": "Places to scrape",
          "type": "array",
          "description": "List of places to scrape",
          "prefill": ["Đà Lạt", "Nha Trang"],
          "editor": "json"
       },
       "type": {
           "title": "Type of places",
           "type": "array",
           "description": "Type of places to scrape",
           "prefill": ["attractions", "restaurants", "hotels"],
           "editor": "json"
       },
       "useApifyProxy": {
          "title": "Apify Proxy",
          "type": "boolean",
          "description": "Use Proxies provided by Apify",
          "default": false
       }
    },
    "required": ["query", "type"]
}
